{
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/forbid-prop-types": "off",
    "react/require-default-props": "off"
  }
}
